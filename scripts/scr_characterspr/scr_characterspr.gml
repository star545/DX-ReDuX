function get_chardetail(_cat, _subcat = -4, chara = -4) {
	if chara == -4 {
		chara = obj_player1.character
		if obj_player1.super_mario chara = characters.mario
		//if (obj_player1.bo_noise && chara == characters.noise) chara = characters.bo_noise	
		// if (obj_player1.isgustavo) chara = characters.gus
		
	}
	
	var char = global.charDetails[chara]
	var cat = variable_struct_get(char, _cat)
	//trace(cat)
	if _subcat != -4 {
		cat = variable_struct_get(cat, _subcat)
		
	}
	//trace(cat)
	var result = 0
	return cat	
	
}


